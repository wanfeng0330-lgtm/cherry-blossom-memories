# 七牛云环境变量配置

# 七牛云配置
QINIU_ACCESS_KEY=your_access_key_here
QINIU_SECRET_KEY=your_secret_key_here
QINIU_BUCKET=your_bucket_name
QINIU_DOMAIN=https://your_domain.com
QINIU_ZONE=z0

# 数据库配置
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/cherry_blossom_memories
USE_MEMORY_DB=false

# 服务器配置
PORT=3001
NODE_ENV=production

# JWT密钥
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# 密码保护
PROTECTION_PASSWORD=your_password_here
