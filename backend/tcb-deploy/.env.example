# CloudBase 环境变量
TENANT_ID=your_tenant_id
SECRET_ID=your_secret_id
SECRET_KEY=your_secret_key
ENV_ID=your_env_id
REGION=ap-guangzhou

# MongoDB
MONGODB_URI=mongodb://username:password@your-mongodb-uri.mongodb.net/cherry-blossom-memories?retryWrites=true&w=majority

# JWT
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# 密码保护（可选）
PROTECTION_PASSWORD=your_password_here

# 腾讯云COS（用于文件存储）
COS_SECRET_ID=your_cos_secret_id
COS_SECRET_KEY=your_cos_secret_key
COS_BUCKET=your_bucket_name
COS_REGION=ap-guangzhou
COS_DOMAIN=https://your_bucket.cos.ap-guangzhou.myqcloud.com
